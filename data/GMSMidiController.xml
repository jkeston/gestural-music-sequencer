<?xml version="1.0" encoding="iso-8859-1"?><!-- XML Document to load external control parameters into  -- The GMS. Use "999" or empty quotes to bypass a controller  -- a "startNote" value in the Transpose tag will enable keyboard  -- Transposition from that note (i.e. 60 for middle C). --><GMSMidiController>	<MIDIControllerName>Korg MS2000</MIDIControllerName>	<MidiChannel CC="31" type="knob" />	<TopOctaveIncrement CC="82" type="button" />	<BottomOctaveIncrement CC="78" type="button" />	<Preset CC="999" useProgramChange="true" />	<BPM CC="999" />	<ToggleFreeMode CC="77" type="button" />	<StartStop CC="89" type="button" />	<Sustain CC="64" invert="false" type="pedal" />	<SetDuration CC="30" type="knob" />	<ToggleDotted CC="92" type="button" />	<SetScale CC="29" type="knob" />	<ToggleMirroring CC="86" type="button" />	<Transpose startNote="48" />	<Randomness CC="20" type="knob" />	<NotePD>		<onOff CC="87" type="button" />		<note1 name="C" CC="23" type="knob" /> 		<note2 name="C#" CC="24" type="knob" /> 		<note3 name="D" CC="25" type="knob" /> 		<note4 name="D#" CC="26" type="knob" /> 		<note5 name="E" CC="73" type="knob" /> 		<note6 name="F" CC="75" type="knob" /> 		<note7 name="F#" CC="70" type="knob" /> 		<note8 name="G" CC="72" type="knob" /> 		<note9 name="G#" CC="27" type="knob" /> 		<note10 name="A" CC="76" type="knob" /> 		<note11 name="A#" CC="12" type="knob" />		<note12 name="B" CC="93" type="knob" />  	</NotePD>	<DurationPD>		<onOff CC="88" type="button" />		<whole CC="14" type="knob" />		<half CC="15" type="knob" />		<quarter CC="18" type="knob" />		<eighth CC="19" type="knob" />		<sixteenth CC="22" type="knob" />		<thirtysecond CC="21" type="knob" />		<sixtyfourth CC="79" type="knob" />		<dotted CC="85" type="knob" />	</DurationPD></GMSMidiController>